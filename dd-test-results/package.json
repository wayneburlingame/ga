{
    "name": "dd-test-results",
    "version": "1.0.0",
    "private": true,
    "description": "Report test results to Datadog",
    "main": "lib/main.js",
    "keywords": [
        "actions",
        "node",
        "datadog",
        "test-results"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zendesk/ga.git"
    },
    "author": "Elvis Porebski <eporebski@zendesk.com>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "tsc -b tsconfig.build.json",
        "format": "prettier --write **/*.ts",
        "lint": "eslint src/**/*.ts",
        "test": "tsc --p tsconfig.build.json --noEmit && jest",
        "all": "npm run build && npm run format && ncc build lib/main.js && npm run lint && npm test"
    },
    "dependencies": {
        "@actions/core": "^1.3.0",
        "@actions/http-client": "^1.0.11",
        "junitxml-to-javascript": "^1.1.4",
        "xml2js": "^0.4.23",
        "xml2js-xpath": "^0.12.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/node": "^12.19.11",
        "@types/xml2js": "^0.4.8",
        "@typescript-eslint/parser": "^4.25.0",
        "eslint": "^7.27.0",
        "eslint-plugin-github": "^4.1.3",
        "eslint-plugin-jest": "^24.3.6",
        "jest": "^26.6.3",
        "jest-circus": "^27.0.1",
        "prettier": "^2.3.0",
        "junitxml-to-javascript": "^1.1.4",
        "ts-jest": "^26.5.6",
        "typescript": "^4.2.4",
        "xml2js": "^0.4.23",
        "xml2js-xpath": "^0.12.0"
    }
}
